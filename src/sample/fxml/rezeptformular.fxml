<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="350.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller.RezeptformularController">
   <children>
      <TextArea fx:id="textEingabe" layoutX="25.0" layoutY="74.0" prefHeight="200.0" prefWidth="414.0" />
      <TextField fx:id="nameEingabe" layoutX="25.0" layoutY="29.0" />
      <TextField fx:id="zutatEingabe" layoutX="25.0" layoutY="296.0" prefHeight="27.0" prefWidth="167.0" />
      <Spinner fx:id="zutatAnzahlEingabe" layoutX="205.0" layoutY="296.0" prefHeight="27.0" prefWidth="62.0" />
      <Button fx:id="zutatHinzufuegenButton" layoutX="285.0" layoutY="296.0" mnemonicParsing="false" onAction="#zutatHinzufuegen" text="+" />
      <Button fx:id="abbrechenButton" layoutX="353.0" layoutY="29.0" mnemonicParsing="false" onAction="#abbrechen" text="abbrechen" />
      <Button fx:id="rezeptButton" layoutX="291.0" layoutY="29.0" mnemonicParsing="false" onAction="#rezeptHinzufuegen" text="add" />
   </children>
</AnchorPane>
